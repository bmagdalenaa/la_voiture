<h1><%= @carModel.car_model_name %></h1>

<h3><%= link_to @manufacturer.manufacturer_name, @manufacturer %></h3>

<div class="title">
  <% if @carModel.image.present? %>
    <h1><%= image_tag @carModel.image.variant(resize_to_limit:[250, 250]) %>
  <% end %>
</div>

<p><%= @carModel.description %></p>

